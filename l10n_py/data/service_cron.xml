<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="account_journal_entry_number_cron" model="ir.cron">
            <field name="name">Enumerate journal entries</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="interval_number">6</field>
            <field name="interval_type">hours</field>
            <field name="state">code</field>
            <field name="interval_number">1</field>
        </record>

        <record id="nota_remision_campos_rg90" model="ir.cron">
            <field name="name">Complete rg90 fields for delivery note</field>
            <field eval="False" name="active" />
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="interval_number">1</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="state">code</field>
            <field name="code">model.rg90_remission_fields()</field>
        </record>

        <record id="invoice_authorization_cron" model="ir.cron">
            <field name="name">Invoice Authorization Is Valid</field>
            <field name="model_id" ref="model_invoice_authorization"/>
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="state">code</field>
            <field name="code">
                env['invoice.authorization'].search([])._compute_is_valid()
            </field>
        </record>

    </data>
</odoo>
