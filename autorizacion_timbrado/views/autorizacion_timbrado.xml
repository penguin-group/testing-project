<odoo>
    <data>
        <record id="autorizacion_timbrado.timbrado_form_view" model="ir.ui.view">
            <field name="name">autorizacion_timbrado.timbrado_form_view</field>
            <field name="model">interfaces_timbrado.timbrado</field>
            <field name="inherit_id" ref="interfaces_timbrado.timbrado_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='inicio_vigencia']" position="after">
                    <field name="nro_autorizacion"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>