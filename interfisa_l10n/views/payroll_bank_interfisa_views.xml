<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="payroll_bank_interfisa_view_tree" model="ir.ui.view">
        <field name="name">payroll.bank.interfisa.list</field>
        <field name="model">payroll.bank.interfisa</field>
        <field name="arch" type="xml">
            <list>
                <field name="name" />
            </list>
        </field>
    </record>
    <record id="payroll_bank_interfisa_view_form" model="ir.ui.view">
        <field name="name">payroll.bank.interfisa.form</field>
        <field name="model">payroll.bank.interfisa</field>
        <field name="arch" type="xml">
            <form string="Banco Interfisa">
                <sheet>
                    <div class="oe_title">
                        <span class="oe_edit_only">Name</span>
                        <h1>
                            <field
                                name="name"
                            />
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
