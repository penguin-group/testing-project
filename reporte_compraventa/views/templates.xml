<odoo>
        <record id="reporte_compra_xlsx_action" model="ir.actions.report">
                <field name="name">Exportar a xlsx</field>
                <field name="model">reporte_compraventa.wizardcompra</field>
                <field name="report_type">xlsx</field>
                <field name="report_name">reporte_compraventa.reporte_compra_xlsx</field>
                <field name="report_file">reporte_compraventa.reporte_compra_xlsx</field>
