        </record>

        <record id="reporte_venta_xlsx_action" model="ir.actions.report">
                <field name="name">Exportar a xlsx</field>
                <field name="model">reporte_compraventa.wizardventa</field>
                <field name="report_type">xlsx</field>
                <field name="report_name">reporte_compraventa.reporte_venta_xlsx</field>
                <field name="report_file">reporte_compraventa.reporte_venta_xlsx</field>
                <field name="print_report_name">'Libro de ventas'</field>
        </record>


</odoo>