<odoo>
    <!-- proveedores_timbrado.timbrado action window -->
    <record id="proveedores_timbrado_action" model="ir.actions.act_window">
        <field name="name">Timbrado</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">proveedores_timbrado.timbrado</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                
            </p>
        </field>
    </record>

    <!-- proveedores_timbrado.timbrado form view -->
    <record id="timbrado_view_form" model="ir.ui.view">
        <field name="name">timbrado</field>
        <field name="model">proveedores_timbrado.timbrado</field>
        <field name="arch" type="xml">
            <form string="Timbrado">
                <sheet>
                    <group>
                       <group>
                            <field name="name"/>
                            <field name="inicio_vigencia"/>
                            <field name="fin_vigencia"/>
                       </group>
                        <group>
                            <field name="rango_inicial"/>
                            <field name="rango_final"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- proveedores_timbrado.timbrado tree view -->
    <record id="timbrado_view_tree" model="ir.ui.view">
        <field name="name">Timbrado</field>
        <field name="model">proveedores_timbrado.timbrado</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="inicio_vigencia"/>
                <field name="fin_vigencia"/>
                <field name="rango_inicial"/>
                <field name="rango_final"/>
            </tree>
        </field>
    </record>
</odoo>