<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_id_Account_move_i1" model="ir.ui.view">
        <field name="name">account.move.inherit.view.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='timbrado_proveedor']" position="replace">
                <!-- Add your fields here -->
                <field context="{'default_partner_id':partner_id}" domain="[('partner_id','=',partner_id)]" invisible="move_type not in ['in_invoice', 'in_refund']" name="timbrado_id" readonly="state not in ['draft']"/>
                <field invisible="timbrado_id == False" name="fecha_inicio"/>
                <field invisible="timbrado_id == False" name="fecha_vencimiento"/>
            </xpath></field>
    </record>
</odoo>
