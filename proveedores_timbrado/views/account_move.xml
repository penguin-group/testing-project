<odoo>
    <record id="view_id_Account_move_i1" model="ir.ui.view">
        <field name="name">account.move.inherit.view.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='timbrado_proveedor']" position="replace">
                <!-- Add your fields here -->
                <field name="timbrado_id" domain="[('partner_id','=',partner_id)]" attrs="{'invisible':[('move_type','not in',['in_invoice','in_refund'])],'readonly':[('state','not in',['draft'])]}"
                       context="{'default_partner_id':partner_id}"/>
                <field name="fecha_inicio" attrs="{'invisible':[('timbrado_id','=',False)]}"/>
                <field name="fecha_vencimiento" attrs="{'invisible':[('timbrado_id','=',False)]}"/>
            </xpath>
        </field>
    </record>
</odoo>