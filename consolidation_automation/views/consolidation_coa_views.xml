<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="consolidation_account_form_inherited" model="ir.ui.view">
            <field name="name">consolidation.account.form.inherited</field>
            <field name="model">consolidation.account</field>
            <field name="inherit_id" ref="account_consolidation.consolidation_account_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='sequence']" position="after">
                    <field name="account_prefix"/>
                </xpath>
            </field>
        </record>
    
    </data>
    

</odoo>
