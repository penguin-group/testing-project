<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_agreement_tag" model="ir.actions.act_window">
        <field name="name">Tags</field>
        <field name="res_model">agreement.tag</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="view_pisa_agreement_tag_form" model="ir.ui.view">
        <field name="name">pisa.agreement.tag.form</field>
        <field name="model">agreement.tag</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="name" />
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>
</odoo>