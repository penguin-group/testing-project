<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- GitHub Data Sync Cron Job -->
        <record id="ir_cron_sync_github_data" model="ir.cron">
            <field name="name">Sync GitHub Data</field>
            <field name="model_id" ref="model_project_project"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_all_github_data()</field>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="priority">5</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(minutes=30)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>
    </data>
</odoo>