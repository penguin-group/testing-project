<odoo>
    <record id="view_hr_reports_mtess_wizard_form" model="ir.ui.view">
        <field name="name">hr.reports.mtess.wizard.form</field>
        <field name="model">hr.reports.mtess.wizard</field>
        <field name="arch" type="xml">
            <form string="MTESS Report Wizard">
                <group>
                    <field name="year"/>
                </group>
                <footer>
                    <button string="Generate Reports" type="object" name="action_generate_reports" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>